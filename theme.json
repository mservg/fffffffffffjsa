{
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "useRootPaddingAwareAlignments": true,
    "color": {
      "custom": true,
      "customDuotone": true,
      "customGradient": true,
      "defaultDuotones": false,
      "defaultGradients": false,
      "defaultPalette": false,
      "palette": [
        {
          "slug": "brand-cream",
          "color": "#fcefbd",
          "name": "Brand Cream"
        },
        {
          "slug": "brand-yellow",
          "color": "#ffe54b",
          "name": "Brand Yellow"
        },
        {
          "slug": "brand-gold",
          "color": "#f0cc00",
          "name": "Brand Gold"
        },
        {
          "slug": "brand-blue",
          "color": "#3269ff",
          "name": "Brand Blue"
        },
        {
          "slug": "brand-indigo",
          "color": "#6476d1",
          "name": "Brand Indigo"
        },
        {
          "slug": "brand-navy",
          "color": "#1c2f7c",
          "name": "Brand Navy"
        },
        {
          "slug": "brand-midnight",
          "color": "#10284a",
          "name": "Brand Midnight"
        },
        {
          "slug": "white",
          "color": "#ffffff",
          "name": "White"
        },
        {
          "slug": "black",
          "color": "#000000",
          "name": "Black"
        },
        {
          "slug": "gray-light",
          "color": "#f8f9fa",
          "name": "Light Gray"
        },
        {
          "slug": "gray-medium",
          "color": "#6c757d",
          "name": "Medium Gray"
        },
        {
          "slug": "gray-dark",
          "color": "#343a40",
          "name": "Dark Gray"
        }
      ],
      "gradients": [
        {
          "slug": "brand-gold-to-blue",
          "gradient": "linear-gradient(135deg, var(--wp--preset--color--brand-gold) 0%, var(--wp--preset--color--brand-blue) 100%)",
          "name": "Gold to Blue"
        },
        {
          "slug": "midnight-overlay",
          "gradient": "linear-gradient(180deg, rgba(16, 40, 74, 0) 0%, rgba(16, 40, 74, 0.8) 100%)",
          "name": "Midnight Overlay"
        }
      ]
    },
    "typography": {
      "customFontSize": true,
      "fontStyle": true,
      "fontWeight": true,
      "letterSpacing": true,
      "textDecoration": true,
      "textTransform": true,
      "dropCap": false,
      "fontSizes": [
        {
          "slug": "small",
          "size": "0.875rem",
          "name": "Small",
          "fluid": {
            "min": "0.8rem",
            "max": "0.875rem"
          }
        },
        {
          "slug": "normal",
          "size": "1rem",
          "name": "Normal",
          "fluid": {
            "min": "0.9rem",
            "max": "1rem"
          }
        },
        {
          "slug": "medium",
          "size": "1.125rem",
          "name": "Medium",
          "fluid": {
            "min": "1rem",
            "max": "1.125rem"
          }
        },
        {
          "slug": "large",
          "size": "1.25rem",
          "name": "Large",
          "fluid": {
            "min": "1.125rem",
            "max": "1.25rem"
          }
        },
        {
          "slug": "x-large",
          "size": "1.5rem",
          "name": "Extra Large",
          "fluid": {
            "min": "1.25rem",
            "max": "1.5rem"
          }
        },
        {
          "slug": "xx-large",
          "size": "2rem",
          "name": "Extra Extra Large",
          "fluid": {
            "min": "1.5rem",
            "max": "2rem"
          }
        },
        {
          "slug": "xxx-large",
          "size": "3rem",
          "name": "Huge",
          "fluid": {
            "min": "2rem",
            "max": "3rem"
          }
        }
      ],
      "fontFamilies": [
        {
          "slug": "epilogue",
          "fontFamily": "Epilogue, sans-serif",
          "name": "Epilogue"
        },
        {
          "slug": "lora",
          "fontFamily": "Lora, serif",
          "name": "Lora"
        },
        {
          "slug": "system",
          "fontFamily": "-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", Arial, sans-serif",
          "name": "System"
        }
      ]
    },
    "spacing": {
      "customSpacingSize": true,
      "spacingScale": {
        "operator": "*",
        "increment": 1.5,
        "steps": 7,
        "mediumStep": 1.5,
        "unit": "rem"
      },
      "spacingSizes": [
        {
          "slug": "10",
          "size": "0.5rem",
          "name": "1"
        },
        {
          "slug": "20",
          "size": "1rem",
          "name": "2"
        },
        {
          "slug": "30",
          "size": "1.5rem",
          "name": "3"
        },
        {
          "slug": "40",
          "size": "2rem",
          "name": "4"
        },
        {
          "slug": "50",
          "size": "3rem",
          "name": "5"
        },
        {
          "slug": "60",
          "size": "4rem",
          "name": "6"
        },
        {
          "slug": "70",
          "size": "5rem",
          "name": "7"
        },
        {
          "slug": "80",
          "size": "6rem",
          "name": "8"
        }
      ],
      "units": [
        "%",
        "px",
        "em",
        "rem",
        "vh",
        "vw"
      ]
    },
    "layout": {
      "contentSize": "1200px",
      "wideSize": "1400px"
    },
    "blocks": {
      "core/button": {
        "border": {
          "radius": true,
          "color": true,
          "style": true,
          "width": true
        },
        "color": {
          "background": true,
          "text": true,
          "gradient": true
        },
        "spacing": {
          "padding": true
        },
        "typography": {
          "fontSize": true,
          "fontWeight": true,
          "textTransform": true,
          "letterSpacing": true
        }
      },
      "core/heading": {
        "typography": {
          "fontSize": true,
          "fontWeight": true,
          "lineHeight": true,
          "letterSpacing": true,
          "textTransform": true
        },
        "color": {
          "text": true,
          "background": true,
          "gradient": true
        }
      },
      "core/paragraph": {
        "typography": {
          "fontSize": true,
          "lineHeight": true
        },
        "color": {
          "text": true,
          "background": true
        }
      }
    }
  },
  "styles": {
    "color": {
      "background": "var(--wp--preset--color--white)",
      "text": "var(--wp--preset--color--gray-dark)"
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--lora)",
      "fontSize": "var(--wp--preset--font-size--normal)",
      "lineHeight": "1.6"
    },
    "spacing": {
      "blockGap": "var(--wp--preset--spacing--30)"
    },
    "elements": {
      "h1": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--xxx-large)",
          "fontWeight": "700",
          "lineHeight": "1.2"
        },
        "color": {
          "text": "var(--wp--preset--color--brand-midnight)"
        }
      },
      "h2": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--xx-large)",
          "fontWeight": "700",
          "lineHeight": "1.3"
        },
        "color": {
          "text": "var(--wp--preset--color--brand-midnight)"
        }
      },
      "h3": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--x-large)",
          "fontWeight": "700",
          "lineHeight": "1.4"
        },
        "color": {
          "text": "var(--wp--preset--color--brand-midnight)"
        }
      },
      "h4": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "700"
        }
      },
      "h5": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--medium)",
          "fontWeight": "700"
        }
      },
      "h6": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--normal)",
          "fontWeight": "700"
        }
      },
      "link": {
        "color": {
          "text": "var(--wp--preset--color--brand-blue)"
        },
        ":hover": {
          "color": {
            "text": "var(--wp--preset--color--brand-gold)"
          }
        },
        ":focus": {
          "outline": {
            "color": "var(--wp--preset--color--brand-yellow)",
            "style": "solid",
            "width": "2px"
          },
          "outlineOffset": "2px"
        }
      },
      "button": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontWeight": "700",
          "textTransform": "uppercase",
          "letterSpacing": "0.5px"
        },
        "color": {
          "background": "var(--wp--preset--color--brand-blue)",
          "text": "var(--wp--preset--color--white)"
        },
        "border": {
          "radius": "4px"
        },
        "spacing": {
          "padding": {
            "top": "0.75rem",
            "right": "2rem",
            "bottom": "0.75rem",
            "left": "2rem"
          }
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--brand-gold)"
          }
        },
        ":focus": {
          "outline": {
            "color": "var(--wp--preset--color--brand-yellow)",
            "style": "solid",
            "width": "2px"
          },
          "outlineOffset": "2px"
        }
      }
    },
    "blocks": {
      "core/site-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontSize": "var(--wp--preset--font-size--large)",
          "fontWeight": "700"
        }
      },
      "core/navigation": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)",
          "fontWeight": "700",
          "textTransform": "uppercase",
          "letterSpacing": "0.5px"
        }
      },
      "core/post-title": {
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--epilogue)"
        }
      }
    }
  },
  "templateParts": [
    {
      "name": "header",
      "title": "Header",
      "area": "header"
    },
    {
      "name": "footer",
      "title": "Footer",
      "area": "footer"
    }
  ],
  "customTemplates": [
    {
      "name": "page-oeuvre",
      "title": "Oeuvre Archive",
      "postTypes": ["page"]
    },
    {
      "name": "page-en-work",
      "title": "English Work Page",
      "postTypes": ["page"]
    }
  ]
}